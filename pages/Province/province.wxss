/**index.wxss**/
.topbox {
  display: flex;
  flex-direction: row;  
  height: 110rpx;
  width: 100%;	
  background-color:  #898989;
}

.topbox-backbtn {
  width:42px;
  height: 37px;
  margin-left: 20rpx;
  align-self: center;
}

.topbox-title {
  width:auto; 
  height: 80%;
  font-size: 23px; 
  margin-left: 150rpx;
  align-self: center;
  line-height: 80rpx;
}

.province-title{
  height: 60rpx;
  display: flex;
  justify-content: center;
  font-size: 20px;
  background-color: #bfbfbf;
}

.city-title{
  height: 40rpx;
  display: flex;
  justify-content: center;
  font-size: 13px;
  background-color: #bfbfbf;
}

.center-view{
  display: flex;
  justify-content: flex-start;
  flex-flow: row wrap;
}

.center-item-view{
  margin-left: 35rpx;
  margin-top: 30rpx;
  width:140rpx;
  height: 75rpx;
  font-size: 15px;
  line-height: 75rpx;
  display: flex;
  justify-content: center;
  background-color: white;
  border-radius: 5px;
}

.center-lastview{
  margin-left: 20%;
  margin-right: 20%;
  margin-top: 40%;
  margin-bottom: 40%;
  width:60%;
  height: 60rpx;
  font-size: 20px;
  display: flex;
  justify-content: center;
  border-style: double;
  border-color: black;
}
